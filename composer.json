{
  "name": "dollmetzer/zzapapp",
  "description": "Lightweight PHP application skeleton",
  "type": "application",
  "license": "GPLv3",
  "version": "3.0.0",
  "authors": [
    {
      "name": "Dirk Ollmetzer",
      "email": "dirk.ollmetzer@ollmetzer.com",
      "homepage": "http://www.ollmetzer.com"
    }
